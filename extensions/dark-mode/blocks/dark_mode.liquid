<h1>hello</h1>
<script>
async function fetchDarkMode() {
  try {
    const response = await fetch("https://ostaddev.myshopify.com/apps/darkmode", {
      method: "GET",
      headers: {
        'Content-Type': "application/json",
      }
    });
    if (!response.ok) {
      throw new Error(HTTP error: ${response.status});
    }
    const data = await response.json();
    return data;
  } catch (error) {
    console.error("Error fetching:", error);
    return null;
  }

fetchDarkMode().then((data) => {
  console.log(data);
}).catch((error) => {
  console.error("fetch failed:", error);
});
}
</script>

