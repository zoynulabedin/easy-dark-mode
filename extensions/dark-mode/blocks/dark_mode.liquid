<h1>hello</h1>
<script>
async function darkMode(){
    try {
        const response = await fetch("https://ostaddev.myshopify.com/apps/darkmode", {
            method: "GET",
            redirect: "manual",
            headers: {
                'Content-Type': "application/json",
                'Access-Control-Allow-Origin': '*',
            }
        });
        console.log(response, 'response');
        if (!response.ok) {
            throw new Error(`HTTP error: ${response.status}`);
        }
        const data = await response.json();
        return data;
    } catch (error) {
        throw error;
    }
}
darkMode().then((data) => {
  console.log(data);
}).catch((error) => {
    // handle the error
});

</script>

